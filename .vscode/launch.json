{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter DEV (debug)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "flutterMode": "debug"
    },
    {
      "name": "Flutter STAGING (debug)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "args": ["--flavor", "staging"],
      "flutterMode": "debug"
    },
    {
      "name": "Flutter PRODUCTION (debug)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": ["--flavor", "production"],
      "flutterMode": "debug"
    },
    {
      "name": "Flutter PRODUCTION (profile)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": ["--flavor", "production"],
      "flutterMode": "profile"
    },
    {
      "name": "Flutter PRODUCTION (release)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": ["--flavor", "production"],
      "flutterMode": "release"
    }
  ]
}

